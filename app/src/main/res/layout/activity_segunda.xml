<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".SegundaActivity">

    <TextView
            android:text="@string/activity_2"
            android:layout_width="wrap_content"
            android:layout_height="26dp"
            android:id="@+id/textView2"
            app:layout_constraintStart_toEndOf="@+id/numeroid"
            android:layout_marginStart="104dp"
            app:layout_constraintTop_toTopOf="@+id/txtNumero"/>
    <TextView
            android:text="@string/n_mero"
            android:layout_width="wrap_content"
            android:layout_height="19dp"
            android:id="@+id/txtNumero" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="68dp"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginStart="44dp"/>
    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/numeroid"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="@+id/txtNumero" app:layout_constraintStart_toEndOf="@+id/txtNumero"
            android:layout_marginStart="48dp"/>
    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/devuelto" app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="44dp"
            android:layout_marginTop="100dp" app:layout_constraintTop_toTopOf="parent"/>
    <FrameLayout
            android:layout_width="395dp"
            android:layout_height="621dp" android:layout_marginTop="200dp"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            android:id="@+id/espacio">

    </FrameLayout>

</android.support.constraint.ConstraintLayout>